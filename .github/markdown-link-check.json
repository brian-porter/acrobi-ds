{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://localhost"
    },
    {
      "pattern": "^mailto:"
    },
    {
      "pattern": "^tel:"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^../../../docs/aae/",
      "replacement": "https://github.com/acrobi/design-system/tree/main/docs/aae/"
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206, 999]
}
